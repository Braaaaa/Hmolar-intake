    "@upstash/redis": "^1.35.3",
    "clsx": "^2.1.1",
    "next": "15.4.6",
    "next-intl": "^4.3.4",
    "prisma": "^6.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.53.0",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
